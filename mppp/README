  /usr/include/mp++/detail/mpfr.hpp
  /usr/include/mp++/detail/quadmath.hpp
  /usr/include/mp++/detail/type_traits.hpp
  /usr/include/mp++/detail/utils.hpp
  /usr/include/mp++/detail/visibility.hpp
  /usr/include/mp++/exceptions.hpp
  /usr/include/mp++/extra/pybind11.hpp
  /usr/include/mp++/integer.hpp
  /usr/include/mp++/mp++.hpp
  /usr/include/mp++/rational.hpp
  /usr/include/mp++/real.hpp
  /usr/include/mp++/real128.hpp
  /usr/include/mp++/type_name.hpp
  /usr/lib/cmake/mp++/FindGMP.cmake
  /usr/lib/cmake/mp++/FindMPFR.cmake
  /usr/lib/cmake/mp++/FindQuadmath.cmake
  /usr/lib/cmake/mp++/mp++-config-version.cmake
  /usr/lib/cmake/mp++/mp++-config.cmake
  /usr/lib/cmake/mp++/mp++_export-release.cmake
  /usr/lib/cmake/mp++/mp++_export.cmake
  /usr/lib/libmp++.dll.a
