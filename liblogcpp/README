  /usr/include/liblogcpp/config.hpp
  /usr/include/liblogcpp/log.hpp
  /usr/include/liblogcpp/log_exception.hpp
  /usr/include/liblogcpp/logcpp-qt.hpp
  /usr/include/liblogcpp/logcpp.hpp
  /usr/include/liblogcpp/logcppversion.hpp
  /usr/include/liblogcpp/logmanip.hpp
