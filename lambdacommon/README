  /usr/include/lambdacommon/documents/document.h
  /usr/include/lambdacommon/exceptions/exceptions.h
  /usr/include/lambdacommon/graphics/color.h
  /usr/include/lambdacommon/lambdacommon.h
  /usr/include/lambdacommon/lambdacommon_exports.h
  /usr/include/lambdacommon/lstring.h
  /usr/include/lambdacommon/path.h
  /usr/include/lambdacommon/resources.h
  /usr/include/lambdacommon/serializable.h
  /usr/include/lambdacommon/system/devices.h
  /usr/include/lambdacommon/system/filesystem/filesystem.h
  /usr/include/lambdacommon/system/os.h
  /usr/include/lambdacommon/system/system.h
  /usr/include/lambdacommon/system/terminal.h
  /usr/include/lambdacommon/system/uri.h
  /usr/include/lambdacommon/utils.h
  /usr/lib/liblambdacommon.dll.a
  /usr/lib/pkgconfig/lambdacommon.pc

