  /usr/share/man/man1/dwggrep.1.gz
  /usr/share/man/man1/dwglayers.1.gz
  /usr/share/man/man1/dwgread.1.gz
  /usr/share/man/man1/dwgrewrite.1.gz
  /usr/share/man/man1/dwgwrite.1.gz
  /usr/share/man/man1/dxf2dwg.1.gz

(libredwg0)
  /usr/bin/cygredwg-0.dll

(libredwg-devel)
  /usr/include/dwg.h
  /usr/include/dwg_api.h
  /usr/lib/libredwg.dll.a
